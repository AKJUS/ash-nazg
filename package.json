{
  "name": "ash-nazg",
  "version": "0.0.1",
  "scripts": {
    "start": "node server.js",
    "watch-server": "nodemon -w server.js -w gh.js -w store.js -w log.js --exec 'npm run start'",
    "build-js": "browserify app.jsx --debug | exorcist public/js/app.js.map > public/js/app.js",
    "watch-js": "nodemon -e jsx,js --watch app.jsx --watch ./components/ --watch ./application/ --watch ./stores/ --watch ./actions/ --exec 'npm run build-js'",
    "build-css": "cleancss -o ./public/css/app.min.css app.css",
    "watch-css": "nodemon --ignore ./public/ -e css --exec 'npm run build-css'",
    "build": "npm run build-css && npm run build-js",
    "watch": "npm run watch-css & npm run watch-js",
    "expose": "ngrok http -subdomain=ashnazg 3043"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/w3c/ash-nazg.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.2.17",
    "babelify": "^6.1.0",
    "browserify": "^10.1.3",
    "clean-css": "^3.2.10",
    "exorcist": "^0.4.0",
    "flux": "^2.0.3",
    "nodemon": "1.3.7",
    "normalize.css": "^3.0.3",
    "react": "^0.13.3",
    "react-router": "1.0.0-beta2",
    "uglifyify": "^3.0.1",
    "isomorphic-fetch": "^2.1.0",
    "ungrid": "^1.0.1"
  },
  "dependencies": {
    "async": "^1.2.1",
    "bl": "^1.0.0",
    "body-parser": "^1.13.1",
    "cookie-parser": "^1.3.5",
    "cradle": "^0.6.9",
    "es6-promise": "^2.3.0",
    "express": "^4.12.4",
    "express-session": "^1.11.3",
    "express-winston": "^0.3.1",
    "object-assign": "^3.0.0",
    "octokat": "^0.4.10",
    "passport": "^0.2.2",
    "passport-github2": "^0.1.9",
    "password-generator": "^1.0.0",
    "serve-static": "^1.9.3",
    "session-file-store": "0.0.13",
    "winston": "^1.0.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "uglifyify"
    ]
  }
}
